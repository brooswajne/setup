{
	"coc.preferences.formatOnSaveFiletypes": [
		"go"
	],

	"diagnostic.errorSign": "●",
	"diagnostic.warningSign": "●",
	"diagnostic.infoSign": "●",
	"diagnostic.hintSign": "●",

	"eslint.autoFixOnSave": true,
	"eslint.validate": [
		"html",
		"javascript",
		"javascriptreact",
		"markdown",
		"svelte",
		"typescript",
		"typescriptreact",
		"vue"
	],

	"git.enableGutters": false,
	"git.addGBlameToBufferVar": true
}
